@model Rehearsal

<div class="container">
    <h1 class="my-4">
        Page Heading
        <small>Secondary Text</small>
    </h1>


    <div class="row">
        <div class="col-md-8">
            <img class="img-fluid" src="@Model.Image" />
        </div>

        <div class="col-md-4">
            <h3 class="my-3">@Model.Description</h3>
            <p>@Model.Description</p>
            <h3 class="my-3">Location</h3>
            <ul>
                <li>@Model.Address.City</li>
                <li>@Model.Address.State</li>
            </ul>
        </div>
    </div>
</div>
